{
	"name": "ovcacik-nuxt",
	"version": "1.0.0",
	"description": "My personal webpage",
	"author": "David Ovčačík",
	"private": true,
	"scripts": {
		"dev": "HOST=0.0.0.0 nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
		"lint-fix": "eslint --ext .js,.vue --fix --ignore-path .gitignore .",
		"deploy:staging": "scp -r dist/* deploy@ovcacik.xyz:/var/www/ovcacik-nuxt/staging",
		"deploy:production": "scp -r dist/* deploy@ovcacik.xyz:/var/www/ovcacik-nuxt/production"
	},
	"lint-staged": {
		"*.{js,vue}": "eslint"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"@nuxtjs/axios": "^5.12.2",
		"@nuxtjs/device": "^2.1.0",
		"@nuxtjs/pwa": "^3.2.2",
		"@nuxtjs/style-resources": "^1.0.0",
		"@pixi/filter-shockwave": "^4.1.5",
		"@pixi/unsafe-eval": "^6.2.2",
		"eventemitter3": "^4.0.7",
		"gsap": "^3.6.0",
		"nuxt": "^2.14.7",
		"pixi.js": "^6.2.2",
		"prob.js": "^1.0.6",
		"webfontloader": "^1.6.28"
	},
	"devDependencies": {
		"@babel/plugin-proposal-optional-chaining": "^7.12.1",
		"@nuxt/types": "^2.14.0",
